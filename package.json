{
  "name": "@distype/lavalink",
  "version": "1.0.0",
  "description": "A lavalink wrapper with native bindings to Distype, as well as other JavaScript and TypeScript Discord libraries.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "build:install": "npm i --also=dev && npm run build",
    "docs": "rm -rf ./docs && typedoc",
    "docs:install": "npm i --also=dev && npm run docs",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prepublishOnly": "npm run build:install",
    "test:build": "npm run build",
    "test:docs": "npm run docs",
    "test:lint": "npm run lint"
  },
  "keywords": [
    "bot",
    "discord",
    "discord-bot",
    "discordapp",
    "distype",
    "lavalink"
  ],
  "homepage": "https://distype.br88c.dev",
  "bugs": {
    "url": "https://github.com/distype/lavalink/issues"
  },
  "license": "Apache-2.0",
  "author": "BR88C",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distype/lavalink.git"
  },
  "dependencies": {
    "@discordjs/collection": "^0.4.0",
    "discord-api-types": "^0.26.1",
    "undici": "^4.12.1",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@br88c/eslint-config": "^1.3.0",
    "@types/node": "^16.11.19",
    "@types/ws": "^8.2.2",
    "discord.js": "^13.5.1",
    "distype": "github:distype/distype#build",
    "eslint": "^8.6.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
  "private": false
}
